{
  "name": "@helpscout/blue",
  "version": "1.8.1",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "license": "MIT",
  "description": "Help Scout's React component library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helpscout/blue.git"
  },
  "author": "Jon Quach <hello@jonquach.com>",
  "contributors": [
    "Nickolas Kenyeres <nickolas@knicklabs.com>",
    "Maxi Ferreira <charca@gmail.com>",
    "Tom Graham",
    "Tom O'Connor",
    "Brett Jones <brettjonesdev@gmail.com>",
    "Lauren Brown",
    "Pierre-Luc Babin",
    "Buzz Usborne <hello@buzzusborne.com>",
    "Alisdair McDiarmid"
  ],
  "scripts": {
    "check:npm": "node scripts/check-npm.js",
    "prestart": "npm run check:npm",
    "build-storybook": "build-storybook",
    "build:css": "node scripts/sass.js",
    "build:es2015": "cross-env NODE_ENV=production babel ./src --ignore *.test.js,tests --out-dir ./dist",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./dist/index.es.js",
    "build:es2015modulesSCSS": "cross-env NODE_ENV=production BABEL_ENV=modules babel ./src/styles/includePaths.js --out-file ./dist/styles/includePaths.es.js",
    "build:copy-files": "babel-node ./scripts/copy-files",
    "build:js": "npm run build:es2015 && npm run build:es2015modules && npm run build:es2015modulesSCSS && npm run build:copy-files",
    "build": "npm run clean && npm run copy:css && npm run build:css && npm run build:js",
    "copy:css": "cpx 'src/styles/**/*.scss' 'dist/styles'",
    "coverage": "nyc report --temp-directory=coverage --reporter=text-lcov | coveralls",
    "clean": "rimraf public && rimraf dist && mkdir dist",
    "dev": "npm run check:npm && npm run test:dev",
    "deploy": "npm run storybook:clean && npm run storybook:build && npm run surge:cname && npm run surge:deploy",
    "lint": "npm run lint:js",
    "lint:js": "eslint src/**/*.js --cache",
    "pretty": "npm run prettier",
    "release": "np --no-yarn --no-publish",
    "release:patch": "npm run release -- patch",
    "release:beta": "npm run release -- prerelease --yolo --any-branch",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 8900",
    "storybook:build": "NODE_ENV=production build-storybook -c .storybook -o public",
    "storybook:clean": "rimraf public",
    "surge:cname": "echo 'hsblue.surge.sh' > public/CNAME",
    "surge:deploy": "surge ./public",
    "test": "npm run test:coverage",
    "test:all": "npm run test",
    "test:coverage": "npm run test:dev -- --coverage",
    "test:dev": "node scripts/test.js --env=jsdom",
    "test:coverage:fast": "npm run test:coverage -- --onlyChanged",
    "version": "npm run build",
    "postversion": "npm publish dist",
    "prettier:base": "prettier \"src/**/*.js\" \"test/**/*.js\" \"stories/**/*.js\" --parser=flow",
    "prettier": "npm run prettier:base -- --write"
  },
  "peerDependencies": {
    "react": "^16 || ^15",
    "react-dom": "^16 || ^15"
  },
  "dependencies": {
    "@helpscout/fancy": "^2.1.1",
    "@helpscout/react-utils": "^1.0.1",
    "@seedcss/seed-alert": "0.0.6",
    "@seedcss/seed-button": "0.0.6",
    "@seedcss/seed-color-scheme": "0.0.6",
    "@seedcss/seed-control": "0.0.6",
    "@seedcss/seed-dash": "0.0.6",
    "@seedcss/seed-family": "0.0.6",
    "@seedcss/seed-grid": "0.0.6",
    "@seedcss/seed-list": "0.0.6",
    "@seedcss/seed-this": "0.0.6",
    "closest": "0.0.1",
    "highlight.js": "^9.12.0",
    "lodash.get": "^4.4.2",
    "path-to-regexp": "^2.4.0",
    "pluralize": "^7.0.0",
    "popper.js": "1.14.3",
    "prop-types": "^15.6.1",
    "react-sortable-hoc": "^0.6.7",
    "react-transition-group": "2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@helpscout/helix": "^0.0.13",
    "@storybook/addon-actions": "^4.0.0-alpha.24",
    "@storybook/addon-options": "^4.0.0-alpha.24",
    "@storybook/cli": "^4.0.0-alpha.24",
    "@storybook/react": "^4.0.0-alpha.24",
    "autoprefixer": "7.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-inline-svg": "0.1.0",
    "chalk": "1.1.3",
    "check-dependencies": "1.1.0",
    "coveralls": "3.0.1",
    "cpx": "1.5.0",
    "cross-env": "5.1.0",
    "css-loader": "^1.0.0",
    "dotenv": "4.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.7.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-scope": "^4.0.0",
    "flow-bin": "^0.83.0",
    "fs-extra": "3.0.1",
    "husky": "1.0.0-rc.13",
    "istanbul-api": "1.2.2",
    "istanbul-reports": "1.1.4",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.2",
    "np": "^3.0.4",
    "nyc": "12.0.2",
    "object-assign": "4.1.1",
    "object-values": "2.0.0",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "postcss-scss": "1.0.2",
    "prettier": "1.12.1",
    "pretty-quick": "^1.4.1",
    "raw-loader": "0.5.1",
    "react": "^16.5.2",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "^16.5.2",
    "react-frame-component": "^1.1.0",
    "react-router": "^4",
    "react-router-dom": "^4",
    "react-test-renderer": "^16.5.2",
    "rgb-hex": "2.1.0",
    "rimraf": "2.6.2",
    "sass-loader": "^7.1.0",
    "sass-pathfinder": "^0.0.5",
    "seed-harvester": "^0.1.5",
    "sinon": "^5.0.10",
    "style-loader": "0.18.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "nyc": {
    "exclude": [
      "config/**/*.js",
      "dist/**/*.js",
      "scripts/**/*.js",
      "stories/**/*.js"
    ],
    "cache": false
  }
}
